public class subtracao {
    public static void main(String[] args) {
        int num1 = 10;
        int num2 = 5;

        int soma = num1 + num2;
        int subtracao = num1 - num2;

        System.out.println("A soma é: " + soma);
        System.out.println("A subtração é: " + subtracao);
    }
}
